{
  "name": "smudgejs",
  "version": "0.0.7",
  "description": "Sketching library for Physically Based Rendering",
  "main": "./dist/smudge.js",
  "types": "./dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.prod.js",
    "np": "np",
    "_all": "concurrently --kill-others -p \"[{name}]\" -n \"smudge,blog\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run smudge\" \"npm run blog\"",
    "_smudge": "webpack-dev-server --open",
    "_smudge_build": "webpack ",
    "_smudge_watch": "webpack --watch",
    "_tsc": "tsc",
    "_blog": "cd docs && bundle exec jekyll serve --drafts --livereload",
    "_open_blog": "sleep 2 && open http://127.0.0.1:4000/smudge/",
    "_api_doc": "typedoc --theme minimal --out ./api_doc/ src/js/index.ts",
    "_api_doc_p5": "typedoc --mode modules --exclude **/node_modules/** --excludeExternals --excludeNotExported --excludePrivate --excludeProtected --stripInternal --out ./docs/api_doc_p5/ --theme minimal --readme none --includeVersion --categoryOrder 'Setup' --categoryOrder 'Materials' --categoryOrder 'Shapes' --categoryOrder 'Unsupported' --categoryOrder '*' ./src/types ./src/js/smudge-p5.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbakse/smudge.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/jbakse/smudge/issues"
  },
  "module": "src/js/index.ts",
  "sideEffects": [
    "./src/js/smudge-p5.ts"
  ],
  "homepage": "https://github.com/jbakse/smudge#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "glslify-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "np": "^6.2.0",
    "raw-loader": "^4.0.0",
    "script-loader": "^0.7.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@petamoriken/float16": "^1.1.1",
    "@types/file-saver": "2.0.1",
    "@types/lodash": "^4.14.149",
    "@types/three": "^0.103.2",
    "@types/voronoi-diagram": "^1.0.0",
    "file-saver": "2.0.2",
    "gl-constants": "^1.0.0",
    "gl-matrix": "^3.2.1",
    "lodash": "^4.17.15",
    "polyline-normals": "^2.0.2",
    "three": "^0.113.2",
    "voronoi-diagram": "^1.0.1"
  }
}
